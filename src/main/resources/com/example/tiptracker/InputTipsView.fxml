<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.tiptracker.controllers.InputTipController" spacing="10">
    <DatePicker fx:id="datePicker" promptText="Date"/>
    <TextField fx:id="amountField" promptText="Amount"/>
    <TextArea fx:id="notesField" promptText="Notes"/>
    <Button text="Add Tip" onAction="#addTip"/>
    <Button text="Back to Main Menu" onAction="#goBackToMainMenu"/>
</VBox>
